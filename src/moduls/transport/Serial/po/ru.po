msgstr "Модем"

msgid "Timeout (sec)"
msgstr "Время ожидания (с)"

msgid "Pre-init delay (sec)"
msgstr "Выдержка пре-инициал. (с)"

msgid "Post-init delay (sec)"
msgstr "Выдержка после-инициал. (с)"

msgid "Initialization string 1"
msgstr "Строка инициализ. 1"

