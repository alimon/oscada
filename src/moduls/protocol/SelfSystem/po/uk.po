msgstr "Отримання запиту: '%s': %d, час: %f мс."

#, c-format
msgid "Check and reforward request: '%s': %d, time: %f ms."
msgstr "Перевірка та переспрямування запиту: '%s': %d, час: %f мс."

#, c-format
msgid "Unpack and load request: '%s': %d, time: %f ms."
msgstr "Розпакування та завантаження запиту: '%s': %d, час: %f мс."

#, c-format
msgid "Process request: '%s', time: %f ms."
msgstr "Обробка запиту: '%s', час: %f мс."

#, c-format
