msgstr "Помилка відкриття таблиці '%s'. БД виключено."

#, c-format
msgid "Table '%s' is not present."
msgstr "Таблиця '%s' відсутня."

#, c-format
msgid "Start transaction error: %s"
msgstr "Помилка запуску транзакції: %s"

#, c-format
msgid "DSQL close transaction error: %s"
msgstr "Помилка закриття транзакції: %s"

#, c-format
msgid "Allocate statement error: (%d)%s"
msgstr "Помилка розміщення запиту: (%d)%s"

#, c-format
msgid "Start transaction error: (%d)%s"
msgstr "Помилка запуску транзакції: (%d)%s"

#, c-format
msgid "DSQL prepare error: (%d)%s"
msgstr "Помилка приготування DSQL: (%d)%s"

