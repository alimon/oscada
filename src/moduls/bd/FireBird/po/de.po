msgid ""
"FireBird address to DB must be written as: \"<file>;<user>;<pass>\".\n"
"Where:\n"
"  file - full path to DB file in form: \"[<host>:]<filePath>\";\n"
"  user - DB user;\n"
"  pass - password of DB user."
msgstr ""
"Die Adresse Datenbank FireBird wird geschrieben als: \"<file>;<user>;<pass>\".\n"
"Wo:\n"
"  file - vollständigen Pfad zur Datei in DB Form: \"[<host>:]<filepath>\";\n"
"  user - Benutzer der Datenbank;\n"
"  pass - Kennwort des Datenbankbenutzers."

#, c-format
msgid "Table '%s' is not present."
msgstr "Tabelle '%s' ist nicht vorhanden."

msgid "Table is empty."
msgstr "Tabelle ist leer."

msgid "Row is not present."
msgstr "Datensatz ist nicht vorhanden."
