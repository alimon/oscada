msgstr "13:Помилка відповіді: Не повна."

msgid "20:Respond length error."
msgstr "20:Помилка довжини пакету модуля."

msgid "21:Invalid module CRC."
msgstr "21:Помилка контрольної суми."

msgid "22:Invalid module response."
msgstr "22:Помилка відповіді модуля."

msgid "10:Transport error: "
msgstr "10:Помилка транспорту: "

msgid "23:Undefined range"
msgstr "23:Невизначений діапазон"

msgid "24:Module out of range"
msgstr "24:Вихід за межі діапазону у модулі"

msgid "25:Command ignored (host watchdog)."
msgstr "25:Команду ігноровано (сторожовий таймер)."

msgid "1:Parameter is disabled."
msgstr "1:Параметр вимкнено."

msgid "2:Acquisition is stopped."
msgstr "2:Збір зупинено."
