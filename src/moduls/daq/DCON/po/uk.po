msgid "DCON client"
msgstr "DCON клієнт"

msgid "Roman Savochenko, Almaz Karimov"
msgstr "Роман Савоченко, Алмаз Каримов"

msgid ""
"Allow realization of DCON client service. Supported I-7000 DCON protocol."
msgstr ""
"Надає реалізацію клієнтського сервісу DCON. Підтримується I-7000 DCON "
"протокол."

msgid "Parameters table"
msgstr "Таблиця параметрів"

msgid "Gather data period (s)"
msgstr "Період збору даних (с)"

msgid "Acquisition schedule"
msgstr "Планування опитування"

msgid "Gather task priority"
msgstr "Рівень пріоритету задачі збору даних"

msgid "Serial transport"
msgstr "Послідовний транспорт"

msgid "Request tries"
msgstr "Спроб запиту"

msgid "Standard"
msgstr "Стандарт"

msgid "Module address"
msgstr "Адреса модуля"

msgid "CRC control"
msgstr "Контроль CRC"

msgid "Host signal"
msgstr "Сигнал головного"

msgid "No signal;HostOK"
msgstr "Немає сигналу;Головний у нормі"

msgid "AI method"
msgstr "Метод AI"

msgid ""
"No AI;1AI (#AA);2AI (#AA);3AI (#AA);4AI (#AA);6AI (#AA);8AI (#AA);10AI "
"(#AA);20AI (#AA);16AI (#AA^AA)"
msgstr ""
"Немає AI;1AI (#AA);2AI (#AA);3AI (#AA);4AI (#AA);6AI (#AA);8AI (#AA);10AI "
"(#AA);20AI (#AA);16AI (#AA^AA)"

msgid "Engineer or percent;Hexadecimal (0000 FFFF);Hexadecimal (8000 7FFF)"
msgstr ""
"Інженерний або відсотковий;Шістнадцятковий (0000 FFFF);Шістнадцятковий (8000 "
"7FFF)"

msgid "AO method"
msgstr "Метод AO"

msgid "No AO;1AO (#AA);2AO (#AA);4AO (#AA)"
msgstr "Немає AO;1AO (#AA);2AO (#AA);4AO (#AA)"

msgid ""
"Engineer (00.000 20.000);Engineer (04.000 20.000);Engineer (00.000 10.000);"
"Engineer (+00.000 +20.000);Engineer (+04.000 +20.000);Engineer (+00.000 "
"+10.000);Engineer (-10.000 +10.000);Engineer (+00.000 +05.000);Engineer "
"(-05.000 +05.000);Percent (+000.00 +100.00);Hexadecimal (000 FFF)"
msgstr ""
"Інженерний (00.000 20.000);Інженерний (04.000 20.000);Інженерний (00.000 "
"10.000);Інженерний (+00.000 +20.000);Інженерний (+04.000 +20.000);Інженерний "
"(+00.000 +10.000);Інженерний (-10.000 +10.000);Інженерний (+00.000 +05.000);"
"Інженерний (-05.000 +05.000);Відсотковий (+000.00 +100.00);Шістнадцятковий "
"(000 FFF)"

msgid "DI method"
msgstr "Метод DI"

msgid ""
"No DI;3DI (@AA);4DI (@AA);7DI (@AA);8DI (@AA);14DI (@AA);16DI (@AA);1DI "
"(@AADI);8DI (@AA,FF00)"
msgstr ""
"Немає DI;3DI (@AA);4DI (@AA);7DI (@AA);8DI (@AA);14DI (@AA);16DI (@AA);1DI "
"(@AADI);8DI (@AA,FF00)"

msgid "DO method"
msgstr "Метод DO"

msgid ""
"No DO;2DO (@AA,0300);3DO (@AA,7);4DO (@AA,F);5DO (@AA,1F);7DO (@AA,7F);8DO "
"(@AA,FF);12DO (@AA,0FFF);13DO (@AA,1FFF);16DO (@AA,FFFF);2DO (^AADOVVV);3DO "
"(^AADOVVV);2DO (@AADO);4DO (@AADO);6DO (@AADODD);2DO (@AADO0D);4DO (@(^)"
"AADO0D);4DO (@AA,0F00);8DO (@AA,FF00)"
msgstr ""
"Немає DO;2DO (@AA,0300);3DO (@AA,7);4DO (@AA,F);5DO (@AA,1F);7DO "
"(@AA,7F);8DO (@AA,FF);12DO (@AA,0FFF);13DO (@AA,1FFF);16DO (@AA,FFFF);2DO "
"(^AADOVVV);3DO (^AADOVVV);2DO (@AADO);4DO (@AADO);6DO (@AADODD);2DO "
"(@AADO0D);4DO (@(^)AADO0D);4DO (@AA,0F00);8DO (@AA,FF00)"

msgid "CI method"
msgstr "Метод CI"

msgid "No CI;2CI (#AA);3CI (#AA)"
msgstr "Немає CI;2CI (#AA);3CI (#AA)"

msgid "Call now. "
msgstr "Виклик зараз. "

#, c-format
msgid "Call by period: %s. "
msgstr "Виклик за періодом: %s. "

#, c-format
msgid "Call next by cron '%s'. "
msgstr "Наступний виклик за cron '%s'. "

#, c-format
msgid "Spent time: %s. "
msgstr "Витрачено часу: %s. "

