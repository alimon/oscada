msgstr "Мінімум"

msgid "Alarm"
msgstr "Сигнал"

msgid ""
"Scale parameter alarm:\n"
"out = if (val>max || val<min) then true; else false;"
msgstr ""
"Сигнал за шкалою параметра:\n"
"out = if (val>max || val<min) then true; else false;"

msgid "Flow"
msgstr "Витрати"

msgid "X position"
msgstr "X позиція"

msgid "K1"
msgstr ""

msgid "K2"
msgstr ""

msgid "K3"
msgstr ""

msgid "K4"
msgstr ""

msgid ""
"Flow calculate per formula:\n"
"f = K1*((K3+K4*x)^K2);"
msgstr ""
"Розрахунок витрат за формулою:\n"
"f = K1*((K3+K4*x)^K2);"

msgid "Summation and multiplication"
msgstr "Сума з множенням"

msgid ""
"Summation and multiplication per formula:\n"
"out = in1_1*in1_2*(in1_3*in1_4+in1_5) + in2_1*in2_2*(in2_3*in2_4+in2_5) +\n"
"      in3_1*in3_2*(in3_3*in3_4+in3_5) + in4_1*in4_2*(in4_3*in4_4+in4_5);"
msgstr ""
"Сума з множенням за формулою:\n"
"out = in1_1*in1_2*(in1_3*in1_4+in1_5) + in2_1*in2_2*(in2_3*in2_4+in2_5) +\n"
"      in3_1*in3_2*(in3_3*in3_4+in3_5) + in4_1*in4_2*(in4_3*in4_4+in4_5);"

msgid "Summation and divide"
msgstr "Сума з поділом"

msgid ""
"Summation and divide per formula:\n"
"out = in1_1*in1_2*(in1_3+in1_4/in1_5) + in2_1*in2_2*(in2_3+in2_4/in2_5) +\n"
"      in3_1*in3_2*(in3_3+in3_4/in3_5) + in4_1*in4_2*(in4_3+in4_4/in4_5);"
msgstr ""
"Сума з поділом за формулою:\n"
"out = in1_1*in1_2*(in1_3+in1_4/in1_5) + in2_1*in2_2*(in2_3+in2_4/in2_5) +\n"
"      in3_1*in3_2*(in3_3+in3_4/in3_5) + in4_1*in4_2*(in4_3+in4_4/in4_5);"

