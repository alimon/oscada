msgid "Simple summator"
msgstr "Простий суматор"

msgid ""
"Simple summator per formula:\n"
"out=in1_1*in1_2+in2_1*in2_2+in3_1*in3_2+in4_1*in4_2+\n"
"    in5_1*in5_2+in6_1*in6_2+in7_1*in7_2+in8_1*in8_2;"
msgstr ""
"Простий суматор за формулою:\n"
"out=in1_1*in1_2+in2_1*in2_2+in3_1*in3_2+in4_1*in4_2+\n"
"    in5_1*in5_2+in6_1*in6_2+in7_1*in7_2+in8_1*in8_2;"

msgid "Input 1."
msgstr "Вхід 1."

msgid "Input 2."
msgstr "Вхід 2."

msgid "Simple multiplicator"
msgstr "Просте множення"

msgid ""
"Simple moltiplicator per formula:\n"
"out=(in1_1*in1_2*in1_3*in1_4*in1_5*in1_6)/(in2_1*in2_2*in2_3*in2_4);"
msgstr ""
"Просте множення за формулою:\n"
"out=(in1_1*in1_2*in1_3*in1_4*in1_5*in1_6)/(in2_1*in2_2*in2_3*in2_4);"

msgid "Input 3."
msgstr "Вхід 3."

msgid "Input 4."
msgstr "Вхід 4."

msgid "Multiplicator+divider"
msgstr "Множення+поділ"

msgid ""
"Multiplicator+divider per formula:\n"
"out=in1_1*in1_2*in1_3*in1_4*in1_5*(in2_1*in2_2*in2_3*in2_4*in2_5+\n"
"          (in3_1*in3_2*in3_3*in3_4*in3_5)/(in4_1*in4_2*in4_3*in4_4*in4_5));"
msgstr ""
"Множення та поділ за формулою:\n"
"out=in1_1*in1_2*in1_3*in1_4*in1_5*(in2_1*in2_2*in2_3*in2_4*in2_5+\n"
"          (in3_1*in3_2*in3_3*in3_4*in3_5)/(in4_1*in4_2*in4_3*in4_4*in4_5));"

msgid "Input 3"
msgstr "Вхід 3"

msgid "Input 5"
msgstr "Вхід 5"

msgid "Exponent"
msgstr "Експонента"

msgid ""
"Exponent per formula:\n"
"out=exp (in1_1*in1_2*in1_3*in1_4*in1_5 +\n"
"         (in2_1*in2_2*in2_3*in2_4*in2_5+in3) / (in4_1*in4_2*in4_3*in4_4*in4_5"
"+in5) );"
msgstr ""
"Експонента за формулою:\n"
"out=exp (in1_1*in1_2*in1_3*in1_4*in1_5 +\n"
"         (in2_1*in2_2*in2_3*in2_4*in2_5+in3) / (in4_1*in4_2*in4_3*in4_4*in4_5"
"+in5) );"

msgid "Power"
msgstr "Степінь"

msgid ""
"Power per formula:\n"
"out=(in1_1*in1_2*in1_3*in1_4*in1_5)^(in2_1*in2_2*in2_3*in2_4*in2_5 +\n"
"\t(in3_1*in3_2*in3_3*in3_4*in3_5)/(in4_1*in4_2*in4_3*in4_4*in4_5));"
msgstr ""
"Степінь за формулою:\n"
"out=(in1_1*in1_2*in1_3*in1_4*in1_5)^(in2_1*in2_2*in2_3*in2_4*in2_5 +\n"
"\t(in3_1*in3_2*in3_3*in3_4*in3_5)/(in4_1*in4_2*in4_3*in4_4*in4_5));"

msgid "Input 1"
msgstr "Вхід 1"

msgid "Condition '<'"
msgstr "Умова '<'"

msgid ""
"Condition '<' per formula:\n"
"out=if( in1<(in2_1*in2_2*in2_3*in2_4) ) then in3_1*in3_2*in3_3*in3_4;\n"
"    else in4_1*in4_2*in4_3*in4_4;"
msgstr ""
"Умова '<' за формулою:\n"
"out=if( in1<(in2_1*in2_2*in2_3*in2_4) ) then in3_1*in3_2*in3_3*in3_4;\n"
"    else in4_1*in4_2*in4_3*in4_4;"

msgid "Condition '>'"
msgstr "Умова '>'"

msgid ""
"Condition '>' per formula:\n"
"out=if( in1>(in2_1*in2_2*in2_3*in2_4) ) then in3_1*in3_2*in3_3*in3_4;\n"
"    else in4_1*in4_2*in4_3*in4_4;"
msgstr ""
"Умова '>' за формулою:\n"
"out=if( in1>(in2_1*in2_2*in2_3*in2_4) ) then in3_1*in3_2*in3_3*in3_4;\n"
"    else in4_1*in4_2*in4_3*in4_4;"

msgid "Input 5."
msgstr "Вхід 5."

msgid "Input 6."
msgstr "Вхід 6."

msgid "Full condition"
msgstr "Повна умова"

msgid ""
"Full condition per formula:\n"
"out = if( in1<(in2_1*in2_2*in2_3*in2_4) )    then in3_1*in3_2*in3_3*in3_4;\n"
"      else if( in1>(in4_1*in4_2*in4_3*in4_4) then in5_1*in5_2*in5_3*in5_4;\n"
"      else in6_1*in6_2*in6_3*in6_4;"
msgstr ""
"Повна умова за формулою:\n"
"out = if( in1<(in2_1*in2_2*in2_3*in2_4) )    then in3_1*in3_2*in3_3*in3_4;\n"
"      else if( in1>(in4_1*in4_2*in4_3*in4_4) then in5_1*in5_2*in5_3*in5_4;\n"
"      else in6_1*in6_2*in6_3*in6_4;"

msgid "Select"
msgstr "Вибір"

msgid "Selector"
msgstr "Вибір"

msgid ""
"Selector per formula:\n"
"out = if( sel = 1 )  then in1_1*in1_2*in1_3*in1_4;\n"
"      if( sel = 2 )  then in2_1*in2_2*in2_3*in2_4;\n"
"      if( sel = 3 )  then in3_1*in3_2*in3_3*in3_4;\n"
"      if( sel = 4 )  then in4_1*in4_2*in4_3*in4_4;"
msgstr ""
"Вибір за формулою:\n"
"out = if( sel = 1 )  then in1_1*in1_2*in1_3*in1_4;\n"
"      if( sel = 2 )  then in2_1*in2_2*in2_3*in2_4;\n"
"      if( sel = 3 )  then in3_1*in3_2*in3_3*in3_4;\n"
"      if( sel = 4 )  then in4_1*in4_2*in4_3*in4_4;"

